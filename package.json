{
  "name": "lumio",
  "description": "Lumio is a Clarity Visual Programming IDE for the Blockstack ecosystem.",
  "author": "Marvin Janssen (http://marvinjanssen.me)",
  "version": "1.0.0",
  "keywords": [
    "Blockstack",
    "Clarity",
    "Lumio",
    "IDE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:repl": "node build_repl.js",
    "clean:repl": "rm public/build/clarity_repl.wasm",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "nyc mocha --require reify",
    "test:watch": "nyc mocha --require reify --watch",
    "jsdoc": "jsdoc src README.md -d docs --verbose",
    "jsdoc:clean": "rm -rf docs/*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "ansi-to-html": "^0.6.14",
    "blockstack": "^21.1.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "reify": "^0.20.12",
    "rollup": "^2.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "shelljs": "^0.8.5",
    "sortablejs": "^1.10.2",
    "svelte": "^3.0.0",
    "svelte-sortablejs": "^0.2.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  }
}
